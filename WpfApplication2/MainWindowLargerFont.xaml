<Window x:Class="WpfApplication2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:custom="clr-namespace:WpfApplication2"
        xmlns:ikriv="clr-namespace:IKriv.Windows.Controls.Behaviors"
        Icon="app_logo_org.ico"
        Title="CARE OSINT Based Social Media Analyzer" WindowState="Maximized" WindowStyle="SingleBorderWindow" mc:Ignorable="d" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" Height="650" Width="825" WindowStartupLocation="CenterScreen">

    <Window.Triggers>
        <EventTrigger RoutedEvent="Loaded">
            <BeginStoryboard>
                <Storyboard Duration="00:00:4" Storyboard.TargetProperty="Opacity">
                    <DoubleAnimation From="0" To="1"/>
                </Storyboard>
            </BeginStoryboard>
        </EventTrigger>
    </Window.Triggers>

    <Window.Resources>
        <ResourceDictionary>
            <BooleanToVisibilityConverter x:Key="BoolToVisConverter" />
            <BitmapImage x:Key="DefaultImage" UriSource="/WpfApplication2;component/Resources/profile_picture.jpg" />
            <ControlTemplate x:Key="TextBoxBaseControlTemplate" TargetType="{x:Type TextBoxBase}">
                <Border Background="{TemplateBinding Background}" 
                x:Name="Bd" BorderBrush="#aaaaaa"
                BorderThickness="{TemplateBinding BorderThickness}" CornerRadius="7">
                    <ScrollViewer x:Name="PART_ContentHost"/>
                </Border>
                <ControlTemplate.Triggers>
                    <Trigger Property="IsEnabled" Value="False">
                        <Setter Property="Background" Value="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" TargetName="Bd"/>
                        <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}"/>
                    </Trigger>
                    <Trigger Property="Width" Value="Auto">
                        <Setter Property="MinWidth" Value="100"/>
                    </Trigger>
                    <Trigger Property="Height" Value="Auto">
                        <Setter Property="MinHeight" Value="20"/>
                    </Trigger>
                </ControlTemplate.Triggers>
            </ControlTemplate>

            <ControlTemplate x:Key="PasswordBoxBaseControlTemplate" TargetType="{x:Type PasswordBox}">
                <Border Background="{TemplateBinding Background}" 
                x:Name="Bd" BorderBrush="#aaaaaa"
                BorderThickness="{TemplateBinding BorderThickness}" CornerRadius="7">
                    <ScrollViewer x:Name="PART_ContentHost"/>
                </Border>
                <ControlTemplate.Triggers>
                    <Trigger Property="IsEnabled" Value="False">
                        <Setter Property="Background" Value="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" TargetName="Bd"/>
                        <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}"/>
                    </Trigger>
                    <Trigger Property="Width" Value="Auto">
                        <Setter Property="MinWidth" Value="100"/>
                    </Trigger>
                    <Trigger Property="Height" Value="Auto">
                        <Setter Property="MinHeight" Value="20"/>
                    </Trigger>
                </ControlTemplate.Triggers>
            </ControlTemplate>

            <Style x:Key="FauxLabel" TargetType="{x:Type TextBox}">
                <Setter Property="Background" Value="Transparent"/>
                <Setter Property="BorderBrush" Value="{x:Null}"/>
                <Setter Property="BorderThickness" Value="4"/>
                <Setter Property="IsTabStop" Value="False"/>
                <Setter Property="IsReadOnly" Value="True" />
                <Setter Property="BorderThickness" Value="0" />
            </Style>

        </ResourceDictionary>

    </Window.Resources>

    <Grid>
        
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        
        <DockPanel LastChildFill="True" Grid.Row="0" Grid.Column="0">

            <Menu DockPanel.Dock="Top">

                <MenuItem Header="_File">
                    <!--<MenuItem Header="_" />-->
                    <MenuItem Click="AboutMenuItem_Click"  Header="_About" />
                    <Separator />
                    <MenuItem Click="ExitMenuItem_Click" Header="_Exit" >
                        <MenuItem.ToolTip>
                            <ToolTip>
                                Closes the application.
                            </ToolTip>
                        </MenuItem.ToolTip>
                    </MenuItem>
                </MenuItem>

                <MenuItem Header="_Configuration">
                    <!--<MenuItem Header="_" />-->
                    <MenuItem Click="openFbSessionId_Click" Header="_Get Facebook Access Token" >

                        <MenuItem.ToolTip>
                            <ToolTip>
                                Get Updated Facebook Access Token And Save it for future use.
                            </ToolTip>
                        </MenuItem.ToolTip>

                    </MenuItem>

                    <!--<MenuItem Header="_" />-->
                    <MenuItem Click="openReportingFileDialog_Click" Header="_Change Reporting File" >

                        <MenuItem.ToolTip>
                            <ToolTip>
                                Change and Save Reporting word file for future use.
                            </ToolTip>
                        </MenuItem.ToolTip>

                    </MenuItem>

                </MenuItem>

                <Menu.Background>
                    <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                        <GradientStop Color="#FF3A60AD" Offset="0.528"/>
                        <GradientStop Color="#FF6A85D8" Offset="0.01"/>
                        <GradientStop Color="#FF3464C4" Offset="1"/>
                        <GradientStop Color="#FF202E7E" Offset="1"/>
                    </LinearGradientBrush>
                </Menu.Background>
            </Menu>

            <Grid>

                <TabControl DockPanel.Dock="Top" Padding="0" Name="parentTab">

                    <TabControl.Resources>
          
                        <Style TargetType="TextBox">
                            <Setter Property="VerticalAlignment" Value="Stretch"></Setter>
                            <Setter Property="HorizontalAlignment" Value="Stretch"></Setter>
                            <Setter Property="AcceptsReturn" Value="True"></Setter>
                            <Setter Property="TextWrapping" Value="WrapWithOverflow"></Setter>
                            <Setter Property="MaxLines" Value="5000"></Setter>
                            <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto"></Setter>
                            <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto"></Setter>
                        </Style>

                        <Style TargetType="TabItem">
     
                            <!--<EventSetter Event="PreviewMouseLeftButtonDown" Handler="aTabClicked" />-->
                            <Setter Property="Template">
                                <Setter.Value>
                                    <ControlTemplate TargetType="TabItem">
                                        <Border Name="Border" BorderThickness="1,1,1,0" BorderBrush="Gainsboro" CornerRadius="4,4,0,0" Margin="2,0">
                                            <ContentPresenter x:Name="ContentSite"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Center"
                                        ContentSource="Header"
                                        Margin="10,2"/>
                                        </Border>
                                        <ControlTemplate.Triggers>
                                            <Trigger Property="IsSelected" Value="True">
                                                <Setter TargetName="Border" Property="Background" Value="#d6d6d6" />
                                            </Trigger>
                                            <Trigger Property="IsSelected" Value="False">
                                                <Setter TargetName="Border" Property="Background" Value="GhostWhite" />
                                            </Trigger>
                                        </ControlTemplate.Triggers>

                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </TabControl.Resources>

                    <TabItem >
                    <TabControl Loaded="tabDynamic_Loaded" DockPanel.Dock="Top" Padding="0" Name="tabDynamic" ItemsSource="{Binding}" SelectionChanged="tabDynamic_SelectionChanged"
                        ikriv:TabContent.IsCached="True">
   
                        <ikriv:TabContent.Template>

                            <DataTemplate>
                                <!--Main Grid showing page......-->
                                <Grid x:Name="aaaaaa" Loaded="MainGrid_Loaded" Tag="{Binding Path=tab_number}"  Initialized="MainGrid_Initialized">
                                    <!--********************************a single search tab template****************************-->
                                    <DockPanel x:Name="searchTab" LastChildFill="True" Grid.Row="0" Grid.Column="0"
                                   Visibility="{Binding Path=searchDivVisbility, Converter={StaticResource BoolToVisConverter} }">

                            <Border DockPanel.Dock="Top" Padding="10"  x:Name="headerBorder" Background="#d6d6d6">

                                            <StackPanel HorizontalAlignment="Stretch" Height="Auto" Orientation="Horizontal">

                                                <Grid HorizontalAlignment="Left" x:Name="awen">

                                                    <TextBox MaxLines="1" AcceptsReturn="False" Template="{StaticResource TextBoxBaseControlTemplate}" Text="{Binding Path=mySearch}" Padding="27,3,23,3" Width="250" Height="Auto" Margin="0,0,0,0" VerticalAlignment="Center" Name="searchTextBox" KeyUp="searchTextBox_KeyUp"  TextChanged="searchTextBox_TextChanged" FontSize="15" Cursor="Pen" />
                                                    <TextBlock IsHitTestVisible="False" Text="Search social media..." VerticalAlignment="Center" HorizontalAlignment="Left" Margin="30,0,0,0" Foreground="DarkGray">
                                            
                                            <TextBlock.Style>
            <Style TargetType="{x:Type TextBlock}">
                <Setter Property="Visibility" Value="Collapsed"/>
                <Style.Triggers>
                    <DataTrigger Binding="{Binding Text, ElementName=searchTextBox}" Value="">
                        <Setter Property="Visibility" Value="Visible"/>
                    </DataTrigger>
                </Style.Triggers>
            </Style>
        </TextBlock.Style>
                                                    </TextBlock>
                                                    <Image Height="20" Margin="5,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Name="searchIcon" MouseLeftButtonUp="searchIcon_MouseLeftButtonUp"  Source="/WpfApplication2;component/Resources/search.png" Stretch="Fill" Width="20" Cursor="Hand" Opacity="0.5"  />
                                                        <Image Height="20" Margin="0,0,5,0" HorizontalAlignment="Right" VerticalAlignment="Center" Name="crossIcon" MouseLeftButtonUp="crossIconSimpleSearch_PreviewMouseDown"  Source="/WpfApplication2;component/Resources/cross.png" Stretch="Fill" Width="20" Cursor="Hand" Opacity="0.5" />

                                                </Grid>

                                                <!--<ComboBox Height="28" Name="comboBox1" Margin="10,0,0,0" Width="120" Text="User" IsEditable="True" />-->

                                                <!--<Image Height="50" Width="50" Name="loading_image" Margin="13,0,0,0" Source="/WpfApplication2;component/Resources/loading_img.gif" Stretch="Fill" Cursor="Hand" />
                                                <Image Height="28" PreviewMouseDown="openFbSessionId_Click" Name="fb_image" Margin="13,0,0,0" Source="/WpfApplication2;component/Resources/fb.png" Stretch="Fill" Width="28" Cursor="Hand" />-->

                                                <TextBlock Visibility="Collapsed" Cursor="Hand" Background="#FFFFFF" Padding="10,6,10,6" Margin="18,0,0,0" PreviewMouseDown="sA1_PreviewMouseLeftButtonDown" VerticalAlignment="Center" Text="Sentimental analysis of search" FontSize="12"></TextBlock>
                                                <Border Visibility="Collapsed" DockPanel.Dock="Top" Padding="10" Background="#d6d6d6">
                                                    <TextBlock Cursor="Hand" Background="#FFFFFF" Padding="10,6,10,6" Foreground="#4099ff" Margin="10,0,0,0" PreviewMouseDown="sA_PreviewMouseLeftButtonDown" VerticalAlignment="Center" Text="Twitter sentimental analysis of search" FontSize="12"></TextBlock>
                                                </Border>

                                            </StackPanel>

                                        </Border>
                 
                                        <!--container in a single tab after search header...-->
                                        <UniformGrid Rows="3" Columns="1" Name="tabContentContainer" Background="#ebebeb" >

                                            <!--row number 1...-->
                                            <UniformGrid Background="White" Margin="0,3,0,0" DockPanel.Dock="Top" Name="row1" Rows="1" Columns="3">

                                                <!--column number 1...-->
                                                <DockPanel Name="innerContainerRow1"  LastChildFill="True">

                                                        <UniformGrid Rows="1" Columns="3" DockPanel.Dock="Top">

                                                            <Label Content=""></Label>
                                                            
                                                            <StackPanel Orientation="Horizontal" Background="#d6d6d6">

                                                                <Image Height="20" Margin="13,0,0,0" Source="/WpfApplication2;component/Resources/fb.png" Stretch="Fill" Width="20" />
                                                                <Label VerticalContentAlignment="Center" Padding="10,2,10,2" Background="#d6d6d6" FontWeight="Bold" VerticalAlignment="Stretch" HorizontalAlignment="Center" Margin="0,2,0,0" Name="fbUsers" Content="Facebook Users"></Label>
                                                                
                                                            </StackPanel>
                                                            
                                                            <StackPanel Orientation="Horizontal" Margin="5,0,10,0" HorizontalAlignment="Right">
                                                            <Image  Source="/WpfApplication2;component/Resources/list_count.png" Stretch="Uniform" Width="20" Height="20"/>
                                                                <Label VerticalContentAlignment="Center" Padding="10,2,10,2" VerticalAlignment="Stretch" HorizontalAlignment="Center" Margin="0,2,0,0" Content="{Binding Path=fbUserListCollectionsCount}"></Label>
                                                            </StackPanel>
                                                            
                                                        </UniformGrid>

                                                            <Grid DockPanel.Dock="Top">

                                                        <!--list and side icon...-->
                                                        <ListView ScrollViewer.HorizontalScrollBarVisibility="Auto" Padding="0" Name="fbUserList" ItemsSource="{Binding Path=fbUserListCollections}"  Background="#d6d6d6" Margin="7,0,7,7" DockPanel.Dock="Top">
                                                            <ListView.Template>
                                                                <ControlTemplate TargetType="{x:Type ListView}">
                                                                    <Border CornerRadius="4" BorderThickness="1" BorderBrush="#d6d6d6" Background="#d6d6d6">
                                                                        <ScrollViewer>
                                                                            <ItemsPresenter/>
                                                                        </ScrollViewer>
                                                                    </Border>
                                                                </ControlTemplate>
                                                            </ListView.Template>

                                                                <ListView.ItemTemplate>
                                                                    <DataTemplate>
                                                                        <Grid>
                                                                            <DockPanel Visibility="{Binding Path=fbUserMoreOptionBehindVisibility,Mode=TwoWay,Converter={StaticResource BoolToVisConverter} }">
                                                                                <Image Margin="5,5,5,5" DockPanel.Dock="Left" x:Name="Image_GridViewColumnName" Width="30" Height="30" Source="{Binding GridViewColumnName_ImageSource}" />

                                                                                <TextBox Style="{StaticResource FauxLabel}" DockPanel.Dock="Left" TextBlock.FontSize="15" TextWrapping="WrapWithOverflow" Margin="0,5,5,5" Text="{Binding GridViewColumnName_LabelContent}" Width="Auto" Height="Auto"  />
                                                                                <!--<Label Margin="0,5,5,5" DockPanel.Dock="Left"  TextBlock.FontSize="14" Width="Auto" Height="Auto"  />-->
                                                                                <Label Content="{Binding GridViewColumnName_ID}" Visibility="Collapsed" />
                                                                            </DockPanel>

                                                                            <TextBlock Text="{Binding Path=fbUserMoreOptionText}" Visibility="{Binding Path=fbUserMoreOptionVisibility,Mode=TwoWay,Converter={StaticResource BoolToVisConverter} }" VerticalAlignment="Center" HorizontalAlignment="Center" TextBlock.FontSize="20" TextWrapping="WrapWithOverflow" Width="Auto" Height="Auto"  ></TextBlock>
                                                                        </Grid>
                                                                    </DataTemplate>
                                                                </ListView.ItemTemplate>

                                                                <ListView.ItemContainerStyle>
                                                                <Style TargetType="ListViewItem">
                                                                    <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
                                                                    <Setter Property="Background" Value="White"/>
                                                                    <Setter Property="Cursor" Value="Hand"/>
                                                                    <EventSetter Event="PreviewMouseLeftButtonDown" Handler="fbUserList_PreviewMouseLeftButtonDown" />
                                                                </Style>
                                                            </ListView.ItemContainerStyle>

                                                        </ListView>

                                                        <Image Margin="5,5,5,5" DockPanel.Dock="Left" x:Name="Image_GridViewColumnName11" Width="142" Height="70" Source="/WpfApplication2;component/Resources/loading_img.gif"
                                                       Visibility="{Binding Path=fbUserLoadingImageVisbility,Mode=TwoWay,Converter={StaticResource BoolToVisConverter} }"/>

                                                            <Label VerticalAlignment="Center" Visibility="{Binding nothingToShowTextForFbUserSearchVisibility,Mode=TwoWay,Converter={StaticResource BoolToVisConverter} }" HorizontalAlignment="Center" Foreground="#999999" Content="Nothing to show..." TextBlock.FontSize="13" />

                                                        </Grid>

                                                </DockPanel>
                                                <!--column number 1 end...-->

                                                <!--column number 2...-->
                                                <DockPanel Name="innerContainerRow2"  LastChildFill="True">
                                                        <UniformGrid Rows="1" Columns="3" DockPanel.Dock="Top">

                                                            <Label Content=""></Label>

                                                            <StackPanel Orientation="Horizontal" Background="#d6d6d6">

                                                                <Image Height="20" Margin="13,0,0,0" Source="/WpfApplication2;component/Resources/fb.png" Stretch="Fill" Width="20" />

                                                                <Label VerticalContentAlignment="Center" Padding="10,2,10,2" Background="#d6d6d6" FontWeight="Bold" VerticalAlignment="Stretch" HorizontalAlignment="Center" Margin="0,2,0,0" Content="Facebook Pages"></Label>

                                                            </StackPanel>
                                                            
                                                            <StackPanel Orientation="Horizontal" Margin="5,0,10,0" HorizontalAlignment="Right">
                                                                <Image  Source="/WpfApplication2;component/Resources/list_count.png" Stretch="Uniform" Width="20" Height="20"/>
                                                                <Label VerticalContentAlignment="Center" Padding="10,2,10,2" VerticalAlignment="Stretch" HorizontalAlignment="Center" Margin="0,2,0,0" Content="{Binding Path=fbPageListCollectionsCount}"></Label>
                                                            </StackPanel>

                                                        </UniformGrid>

                                                        <Grid DockPanel.Dock="Top">

                                                            <!--list and side icon-->
                                                        <ListView ScrollViewer.HorizontalScrollBarVisibility="Auto" ItemsSource="{Binding fbPageListCollections}" Background="#d6d6d6" Margin="7,0,7,7" DockPanel.Dock="Top">
                                                            <ListView.Template>
                                                                <ControlTemplate TargetType="{x:Type ListView}">
                                                                    <Border CornerRadius="4" BorderThickness="1" BorderBrush="#d6d6d6" Background="#d6d6d6">
                                                                        <ScrollViewer>
                                                                            <ItemsPresenter/>
                                                                        </ScrollViewer>
                                                                    </Border>
                                                                </ControlTemplate>
                                                            </ListView.Template>

                                                                <ListView.ItemTemplate>
                                                                    <DataTemplate>
                                                                        <Grid>
                                                                            <StackPanel Visibility="{Binding Path=fbPageMoreOptionBehindVisibility,Mode=TwoWay,Converter={StaticResource BoolToVisConverter} }" Orientation="Horizontal" >
                                                                                <Image Margin="5,5,5,5" x:Name="Image_GridViewColumnName" Width="30" Height="30" Source="{Binding fbPage_ImageSource}" />
                                                                                <TextBox Style="{StaticResource FauxLabel}" DockPanel.Dock="Left" TextBlock.FontSize="15" TextWrapping="WrapWithOverflow" Margin="0,5,5,5" Text="{Binding fbPage_LabelContent}" Width="Auto" Height="Auto"  />
                                                                                <!--<Label Margin="0,5,5,5" Content="{Binding fbPage_LabelContent}" TextBlock.FontSize="14" Width="Auto" Height="Auto"  />-->
                                                                                <Label Content="{Binding fbPage_ID}" Visibility="Collapsed" />
                                                                            </StackPanel>
                                                                            <TextBlock Text="{Binding Path=fbPageMoreOptionText}" Visibility="{Binding Path=fbPageMoreOptionVisibility,Mode=TwoWay,Converter={StaticResource BoolToVisConverter} }" VerticalAlignment="Center" HorizontalAlignment="Center" TextBlock.FontSize="20" TextWrapping="WrapWithOverflow" Width="Auto" Height="Auto"  ></TextBlock>
                                                                        </Grid>
                                                                    </DataTemplate>
                                                                </ListView.ItemTemplate>

                                                                <ListView.ItemContainerStyle>
                                                                <Style TargetType="ListViewItem">
                                                                    <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
                                                                    <Setter Property="Background" Value="White"/>
                                                                    <Setter Property="Cursor" Value="Hand"/>
                                                                    <EventSetter Event="PreviewMouseLeftButtonDown" Handler="fbPageList_PreviewMouseLeftButtonDown" />
                                                                </Style>
                                                            </ListView.ItemContainerStyle>

                                                        </ListView>

                                                        <Image Margin="5,5,5,5" DockPanel.Dock="Left" x:Name="Image_GridViewColumnName22" Width="142" Height="70" Source="/WpfApplication2;component/Resources/loading_img.gif" 
                                                       Visibility="{Binding Path=fbPageLoadingImageVisbility, Converter={StaticResource BoolToVisConverter} }"/>

                                                            <Label VerticalAlignment="Center" Visibility="{Binding nothingToShowTextForFbPageSearchVisibility,Mode=TwoWay,Converter={StaticResource BoolToVisConverter} }" HorizontalAlignment="Center" Foreground="#999999" Content="Nothing to show..." TextBlock.FontSize="13" />
                                                    </Grid>

                                                </DockPanel>
                                                <!--column number 2 end...-->

                                                <!--column number 3...-->
                                                <DockPanel Name="innerContainerRow3"  LastChildFill="True">
                                                       
                                                    <UniformGrid Rows="1" Columns="3" DockPanel.Dock="Top">

                                                            <Label Content=""></Label>

                                                            <StackPanel Orientation="Horizontal" Background="#d6d6d6">

                                                                <Image Height="20" Margin="13,0,0,0" Source="/WpfApplication2;component/Resources/fb.png" Stretch="Fill" Width="20" />

                                                                <Label VerticalContentAlignment="Center" Padding="10,2,10,2" Background="#d6d6d6" FontWeight="Bold" VerticalAlignment="Stretch" HorizontalAlignment="Center" Margin="0,2,0,0" Content="Facebook Groups"></Label>

                                                            </StackPanel>

                                                            <StackPanel Orientation="Horizontal" Margin="5,0,10,0" HorizontalAlignment="Right">
                                                                <Image  Source="/WpfApplication2;component/Resources/list_count.png" Stretch="Uniform" Width="20" Height="20"/>
                                                                <Label VerticalContentAlignment="Center" Padding="10,2,10,2" VerticalAlignment="Stretch" HorizontalAlignment="Center" Margin="0,2,0,0" Content="{Binding Path=fbGroupListCollectionsCount}"></Label>
                                                            </StackPanel>

                                                        </UniformGrid>


                                                        <Grid>
                                                        <!--list and side icon-->
                                                        <ListView ScrollViewer.HorizontalScrollBarVisibility="Auto" ItemsSource="{Binding fbGroupListCollections}" Background="#d6d6d6" Margin="7,0,7,7" DockPanel.Dock="Top">

                                                            <ListView.Template>
                                                                <ControlTemplate TargetType="{x:Type ListView}">
                                                                    <Border CornerRadius="4" BorderThickness="1" BorderBrush="#d6d6d6" Background="#d6d6d6">
                                                                        <ScrollViewer>
                                                                            <ItemsPresenter/>
                                                                        </ScrollViewer>
                                                                    </Border>
                                                                </ControlTemplate>
                                                            </ListView.Template>

                                                                <ListView.ItemTemplate>
                                                                    <DataTemplate>
                                                                        <Grid>
                                                                            <StackPanel Orientation="Horizontal" >
                                                                                <Image Visibility="{Binding Path=fbGroupMoreOptionBehindVisibility,Mode=TwoWay,Converter={StaticResource BoolToVisConverter} }" Margin="5,5,5,5" x:Name="Image_GridViewColumnName" Width="30" Height="30" Source="{Binding fbGroup_ImageSource}" />
                                                                                <TextBox Style="{StaticResource FauxLabel}" DockPanel.Dock="Left" TextBlock.FontSize="15" TextWrapping="WrapWithOverflow" Margin="0,5,5,5" Text="{Binding fbGroup_LabelContent}" Width="Auto" Height="Auto"  />
                                                                                <!--<Label Margin="0,5,5,5" Content="{Binding fbGroup_LabelContent}" TextBlock.FontSize="14" Width="Auto" Height="Auto"  />-->
                                                                                <Label Content="{Binding fbGroup_ID}" Visibility="Collapsed" />
                                                                            </StackPanel>
                                                                            <TextBlock Text="{Binding Path=fbGroupMoreOptionText}" Visibility="{Binding Path=fbGroupMoreOptionVisibility,Mode=TwoWay,Converter={StaticResource BoolToVisConverter} }" VerticalAlignment="Center" HorizontalAlignment="Center" TextBlock.FontSize="20" TextWrapping="WrapWithOverflow" Width="Auto" Height="Auto"  ></TextBlock>
                                                                        </Grid>
                                                                    </DataTemplate>

                                                                </ListView.ItemTemplate>

                                                                <ListView.ItemContainerStyle>
                                                                <Style TargetType="ListViewItem">
                                                                    <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
                                                                    <Setter Property="Background" Value="White"/>
                                                                    <Setter Property="Cursor" Value="Hand"/>
                                                                    <EventSetter Event="PreviewMouseLeftButtonDown" Handler="fbGroupList_PreviewMouseLeftButtonDown" />
                                                                </Style>
                                                            </ListView.ItemContainerStyle>

                                                        </ListView>

                                                        <Image Margin="5,5,5,5" DockPanel.Dock="Left" x:Name="Image_GridViewColumnName33" Width="142" Height="70" Source="/WpfApplication2;component/Resources/loading_img.gif" 
                                                       Visibility="{Binding Path=fbGroupLoadingImageVisbility, Converter={StaticResource BoolToVisConverter} }"/>

                                                            <Label VerticalAlignment="Center" Visibility="{Binding nothingToShowTextForFbGroupSearchVisibility,Mode=TwoWay,Converter={StaticResource BoolToVisConverter} }" HorizontalAlignment="Center" Foreground="#999999" Content="Nothing to show..." TextBlock.FontSize="13" />
                                                    </Grid>
                                                </DockPanel>
                                                <!--column number 3 end...-->

                                            </UniformGrid>
                                            <!--row number 1 end...-->

                                            <!--row number 2-->
                                            <UniformGrid Background="White" Margin="0,3,0,0" DockPanel.Dock="Top" Name="row2" Rows="1" Columns="3">

                                                <!--row number 2-->

                                                <!--column number 1...-->
                                                <DockPanel Name="innerContainerRow4"  LastChildFill="True">
                                                        <UniformGrid Rows="1" Columns="3" DockPanel.Dock="Top">

                                                            <Label Content=""></Label>

                                                            <StackPanel Orientation="Horizontal" Background="#d6d6d6">

                                                                <Image Height="20" Margin="13,0,0,0" Source="/WpfApplication2;component/Resources/twitter.png" Stretch="Fill" Width="20" />

                                                                <Label VerticalContentAlignment="Center" Padding="10,2,10,2" Background="#d6d6d6" FontWeight="Bold" VerticalAlignment="Stretch" HorizontalAlignment="Center" Margin="0,2,0,0" Content="Twitter Users"></Label>

                                                            </StackPanel>
                                                            
                                                            <StackPanel Orientation="Horizontal" Margin="5,0,10,0" HorizontalAlignment="Right">
                                                                <Image  Source="/WpfApplication2;component/Resources/list_count.png" Stretch="Uniform" Width="20" Height="20"/>
                                                                <Label VerticalContentAlignment="Center" Padding="10,2,10,2" VerticalAlignment="Stretch" HorizontalAlignment="Center" Margin="0,2,0,0" Content="{Binding Path=twitterUserListCollectionsCount}"></Label>
                                                            </StackPanel>

                                                        </UniformGrid>

                                                        <Grid>
                                                        <!--list and side icon-->
                                                        <ListView x:Name="twitterUserList"  ItemsSource="{Binding twitterUserListCollections}" Background="#d6d6d6" Margin="7,0,7,7" DockPanel.Dock="Top">

                                                                <ListView.ItemTemplate>
                                                                    
                                                                                <DataTemplate >
                                                                                    <Grid HorizontalAlignment="Stretch">
                                                                                        <StackPanel Visibility="{Binding Path=twitterUserMoreOptionBehindVisibility,Mode=TwoWay,Converter={StaticResource BoolToVisConverter} }" Orientation="Vertical">
                                                                                            <StackPanel Orientation="Horizontal">
                                                                                                <Image Margin="5,5,5,5" Width="30" Height="30" Source="{Binding GridViewColumnName_ImageSource, TargetNullValue={StaticResource DefaultImage}}" />
                                                                                                <StackPanel Orientation="Vertical">
                                                                                        <TextBox Style="{StaticResource FauxLabel}" Margin="0,5,5,0" TextWrapping="WrapWithOverflow" Text="{Binding GridViewColumnName_LabelContent}" TextBlock.FontSize="15" Width="Auto" Height="Auto"  />
                                                                                                    <Label Foreground="#999999"  Margin="0,0,5,0" Visibility="Collapsed" Content="{Binding GridViewColumnName_LabelContentScreenName}"/>
                                                                                                  
                                                                                                    <StackPanel Orientation="Horizontal" Visibility="Collapsed">
                                                                                                        <Label Margin="0,0,0,0" Content="id=" />
                                                                                                        <Label Margin="-5,0,0,0" Content="{Binding GridViewColumnName_ID}" />
                                                                                                    </StackPanel>
                                                                                                </StackPanel>
